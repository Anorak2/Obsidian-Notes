views:
  - type: table
    name: Table
    filters:
      and:
        - file.ext != "jpeg"
        - file.ext != "png"
    order:
      - file.name
      - file.mtime
      - file.ctime
      - file.size
    sort:
      - property: file.ctime
        direction: DESC
      - property: file.mtime
        direction: DESC
    columnSize:
      file.mtime: 212
      file.ctime: 207
