views:
  - type: table
    name: Table
    filters:
      or:
        - file.inFolder("6 - Main Notes")
        - file.inFolder("Math")
        - file.inFolder("Programming")
    order:
      - file.name
      - file.mtime
      - file.ctime
      - file.size
    sort:
      - property: file.mtime
        direction: DESC
      - property: file.ctime
        direction: DESC
    columnSize:
      file.mtime: 212
